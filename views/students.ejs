<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <body>
    <h1>data will be here</h1>
    <table border="1">
      <tr>
        <td>Name</td>
        <td>Age</td>
        <td>Email</td>
        <td>Delete Operation</td>
        <td>Update Operation</td>
      </tr>

      <% students.forEach(student =>{ %>
      <tr>
        <td><%= student.name %></td>
        <td><%= student.age %></td>
        <td><%= student.email %></td>
        <td><a href="/ui/delete/<%= student._id %>">Delete</td>
        <td><a href="/ui/student/<%= student._id %>">Update</td>
      </tr>

      <% }) %>
    </table>
  </body>
</html>
